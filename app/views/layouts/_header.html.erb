<div class="container_top">
  <ul>
    <li><%= link_to "Home", root_path %></li>
    <li><%= link_to "Help", help_path %></li>
    <% if logged_in? %>
      <li style="float: left;"><%= link_to "Log out", logout_path %></li>
      <li style="float: left;"><%= link_to "Settings", edit_user_path(current_user) %></li>
