<h1>Log in</h1>
<%= simple_form_for(:session, url: login_path, multipart: true, class: ".form-horizontal") do |f| %>
	<%= f.input :email %><br />
	<%= f.input :password %><br />
	<div id="remember_me">
		<%= f.label :remember_me, class: "checkbox inline" do %>
		  <%= f.check_box :remember_me %>
		  <span>Remember me on this computer</span>
		<% end %>
		<br /><br />		
	</div>

	<%= f.submit "Log In" %>
<% end %>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>	
<p>Forgot Password? <%= link_to "Reset Password", new_password_reset_path %></p>
