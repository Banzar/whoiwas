<div class="left_column">
<h1>Log in</h1>
<%= simple_form_for(:session, url: login_path) do |f| %>
	<%= f.input :email %><br />
	<%= f.input :password %><br />
	<%= link_to "(forgot password)", new_password_reset_path %>
	<%= f.submit "Log In" %>
<% end %>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>	
</div>
<div class="clear"></div>
