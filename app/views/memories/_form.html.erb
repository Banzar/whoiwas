<div class="container_body">
	<div class="content_body">
		<%= simple_form_for(@memory) do |f| %>
		  <%= f.error_notification %>

		  <div class="form-inputs">
		  	<%= f.input :photo, as: :file %>
		    <%= f.input :legacy_id %>
		  </div>

		  <div class="form-actions">
		    <%= f.button :submit %>
		  </div>
		<% end %>
	</div>
</div>

